{{#partial "contenido"}}
<!DOCTYPE html>
<html>
<meta charset="UTF-8">
<form method="POST" action="/login">
<link rel="stylesheet" href="/css/login.css"> 
<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
<script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
 
    <div class="container-fluid login-view">
  
   <div class="col-md-6 col-md-offset-3">
        <div class="loginContainer">
        <div class = "jumbotron loginBox"
            <div class="centerContent">
                <div class="logoContainer">
                    <!-- <img src="assets/images/goforeat.png"> -->
                    <h1 class="loveat">EnergiApp</h1>
                </div>
                    <p class="">Inicie sesion para comenzar</p>
                    <div class="form-group has-feedback">
                        <input class="form-control" placeholder="Usuario" name="nombreUsuario">
                        <span class="glyphicon glyphicon-user form-control-feedback"></span>
                    </div>
                    <div class="form-group has-feedback">
                    
                        <input type="password" class="form-control" placeholder="Contraseña" name="contrasenia">
                        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                    </div>
                    <div class="row">
                        <div class="col-xs-8">
                        </div>
                        <div class="col-xs-4">
                        
                        <button class="btn btn-success btn-block btn-flat" name="button">Ingresar</button>
                           
                             
                          </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
</div>


